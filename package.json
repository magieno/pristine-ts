  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test",
    "e2e": "cd tests/e2e && jest && cd ..",
    "perf": "cd tests/perf && jest && cd ..",
    "bump-patch": "lerna version patch --yes",
    "publish-ci": "lerna publish from-git --yes --no-verify-access",
