{
  "name": "@pristine-ts/perf",
  "version": "1.0.0",
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "strictNullChecks": false
        }
      }
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec|perf))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  }
}
